items.PortalKey_Hederas_Lair.description = <msg key="server.items.PortalKey.description"/>\n\n<i>Few have experienced the terrors that lurk in Orbis' murkier depths.</i>
items.PortalKey_Henges.name = Fragment: Orbis - Silent Henges
items.PortalKey_Henges.description = <msg key="server.items.PortalKey.description"/>\n\n<i>Mysterious henges tower over a frozen land.</i>
